    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "typescript": "^5.2.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "express": "^4.22.1",
    "tsx": "^4.20.5",
    "yaml": "^2.8.0"
  }
}
